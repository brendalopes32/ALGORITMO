Algoritmo "EXE067"

Var
   N, C: inteiro
Inicio
      Escreva("Digite um valor inteiro e positivo: ")
      Leia(N)
      Se (N > 0) entao
         Escreva("Contagem: ")
            Para C <- 0 ate N faca
                 Escreva(C,",")
            FimPara
            Escreva(" FIM!")
      FimSe
 Fimalgoritmo